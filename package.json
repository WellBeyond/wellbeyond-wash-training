  },
  "scripts": {
    "lint:fix": "eslint src/**/*.js --fix",
    "start": "ionic serve ./custom-sw.js",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "ionic build && npm run build-sw && npm run clean-cra-sw",
    "deploy": "firebase deploy --only hosting:wellbeyond-wash-training",
    "deploy-functions": "firebase deploy --only functions",
    "deploy-rules": "firebase deploy --only firestore:rules",
    "preview": "firebase hosting:channel:deploy frontend-preview",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "resources": "cordova-res ios && cordova-res android && node tools/resources.js",
    "translate": "npx json-autotranslate -i public/locales -t keybased -m i18next -c .firebase/private-key.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An Ionic project",
  "devDependencies": {
    "@capacitor/cli": "2.4.0",
    "@testing-library/react": "^9.5.0",
    "@types/googlemaps": "^3.43.3",
    "firebase-admin": "^9.11.1",
    "intercom-client": "^2.11.2",
    "typescript": "^3.9.10"
  }
